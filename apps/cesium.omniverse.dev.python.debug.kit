[package]
title = "Cesium For Omniverse Python Debugging App"
version = "0.0.0"
app = true

[dependencies]
"cesium.omniverse.dev" = {}
"omni.kit.debug.vscode" = {}
"omni.kit.debug.python" = {}

[settings]
app.window.title = "Cesium For Omniverse Python Debugging App"

[settings.app.exts]
folders.'++' = [
    "${app}", # Find other applications in this folder
    "${app}/exts", # Find extensions in this folder
    "${app}/../exts", # Find cesium.omniverse
    "${app}/../extern/nvidia/app/extscache" # Find omni.kit.window.material_graph
]
